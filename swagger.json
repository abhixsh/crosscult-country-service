{
    "openapi": "3.0.0",
    "info": {
        "title": "Country API",
        "version": "1.0.0",
        "description": "API to manage countries, history, traditions, and food"
    },
    "paths": {
        "/create": {
            "post": {
                "summary": "Create a new country",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    },
                                    "image": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Country created successfully"
                    }
                }
            }
        }
    }
}