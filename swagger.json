                        "in": "body",
                        "description": "Name of the country",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "image",
                        "in": "body",
                        "description": "Image URL of the country",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "description",
                        "in": "body",
                        "description": "Description of the country",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "historyId",
                        "in": "body",
                        "description": "ID of the history document to link",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "traditionsId",
                        "in": "body",
                        "description": "ID of the traditions document to link",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "foodId",
                        "in": "body",
                        "description": "ID of the food document to link",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Country created successfully",
                        "schema": {
                            "$ref": "#/definitions/Country"
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    }
                }
            },
            "get": {
                "summary": "Get all countries",
                "description": "Retrieves all countries with linked history, traditions, and food.",
                "tags": [
                    "Countries"
                ],
                "responses": {
                    "200": {
                        "description": "Successfully retrieved list of countries",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Country"
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    }
                }
            }
